<%- include("partials/header"); %>

<% Object.keys(blog).forEach(function(key){ %>
<% var blg = blog[key]; %>
<div class="container">
    <div class="row">
        <div class="col-md-2">
            <div style="position: fixed;">
                <p>Likes : <%=blg.likes %></p>
                <p>Author : <%=blg.author %></p>
            </div>
        </div>
        <div class="col-md-8">
            <h1><%= blg.title %></h1>
            <br><br><br>
            
            <p><%= blg.body %></p>
        </div>
        <div class="col-md-2"></div>
    </div>
</div>

<% }); %>

<%- include("partials/footer"); %>